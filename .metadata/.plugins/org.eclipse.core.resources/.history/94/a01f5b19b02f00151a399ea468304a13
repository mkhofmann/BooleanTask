
public class ModelCommand{
	public Model model;
	private String command;
    public ModelCommand(Model m, String s) {
        this.model = m;
        this.command = s;
    }
    
	public String getCommand() {
		return command;
	}
	public void setCommand(String str) {
		//TODO: Lock command settings
		this.command = str;
	}
}
